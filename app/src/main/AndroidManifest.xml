<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tracepassenger.app">

    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <application
        android:fullBackupContent="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name="com.orm.SugarApp"
        android:theme="@style/AppTheme">

        <meta-data android:name="DATABASE" android:value="tracepassenger.db" />
        <meta-data android:name="VERSION" android:value="3" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.tracepassenger.domain" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddEditGroup"></activity>
        <activity android:name=".ViewPassengersGroup"></activity>
    </application>

</manifest>